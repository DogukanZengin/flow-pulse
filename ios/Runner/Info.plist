<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Flow Pulse</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>flow_pulse</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	
	<!-- Privacy - Calendar Usage Description -->
	<key>NSCalendarsUsageDescription</key>
	<string>FlowPulse needs access to your calendar to schedule focus sessions and sync your productivity schedule with your existing calendar events.</string>
	
	<!-- Privacy - Camera Usage Description -->
	<key>NSCameraUsageDescription</key>
	<string>FlowPulse uses the camera to let you capture and attach images to your tasks and notes for better organization.</string>
	
	<!-- Privacy - Photo Library Usage Description -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>FlowPulse needs access to your photo library to let you select and attach images to your tasks and notes.</string>
	
	<!-- Privacy - Microphone Usage Description -->
	<key>NSMicrophoneUsageDescription</key>
	<string>FlowPulse uses the microphone to provide audio feedback and voice notes for your productivity sessions.</string>
	
	<!-- Privacy - Notifications Usage Description -->
	<key>NSUserNotificationsUsageDescription</key>
	<string>FlowPulse sends notifications to remind you about focus sessions, breaks, and productivity milestones.</string>
	
	<!-- Privacy - Face ID Usage Description -->
	<key>NSFaceIDUsageDescription</key>
	<string>FlowPulse uses Face ID to securely protect your productivity data and statistics.</string>
	
	<!-- Background Processing for Timer and Audio -->
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
		<string>fetch</string>
	</array>
</dict>
</plist>
